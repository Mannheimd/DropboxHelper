<Window x:Class="DropboxHelper.BrowserWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:DropboxHelper"
        mc:Ignorable="d"
        Title="DropboxLogin" Height="800" Width="600">
    <Grid>
        <WebBrowser x:Name="Browser" Margin="0" HorizontalAlignment="Stretch" VerticalAlignment="Stretch" Navigating="BrowserNavigating"/>
    </Grid>
</Window>
